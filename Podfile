project 'SmartOrder.xcodeproj'

# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'SmartOrder' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SmartOrder

  pod 'Firebase/Core'           #主要函式庫與分析
  pod 'Firebase/AdMob'          #AdMob廣告
  pod 'Firebase/Messaging'	    #雲消息傳遞
  pod 'Firebase/Database'	      #實時數據庫
  pod 'Firebase/Invites'        #邀請
  pod 'Firebase/DynamicLinks'   #動態鏈接
  pod 'Firebase/RemoteConfig'   #遠程配置
  pod 'Firebase/Auth'           #認證
  pod 'GoogleSignIn'            #google登入
  pod 'FBSDKLoginKit'           #FB登入
  pod 'FirebaseUI'
  pod 'FirebaseUI/Google'
  pod 'FirebaseUI/Facebook'
  pod 'FirebaseUI/Phone'
  pod 'Firebase/Storage'	      #存儲
  pod 'Firebase/Performance'	  #性能監控
  pod 'Firebase/Firestore'     	#雲Firestore
  pod 'Firebase/Functions'    	#Firebase Client SDK的雲功能
  pod 'Charts'                    #繪圖


end
